/* SuperSized Plugin */
img { border:none; }
#supersized-loader { position:absolute; top:50%; left:50%; z-index:0; width:60px; height:60px; margin:-30px 0 0 -30px; text-indent:-999em; background:url(../img/progress.gif) no-repeat center center;}
#supersized {  display:block; position:fixed; left:60px; top:0; overflow:hidden; z-index:-999; height:100%; width:100%; }
#supersized img { top: 0px !important; width:auto; height:auto; position:relative; display:none; outline:none; border:none; }
#supersized.speed img { -ms-interpolation-mode:nearest-neighbor; image-rendering: -moz-crisp-edges; } /*Speed*/
#supersized.quality img { -ms-interpolation-mode:bicubic; image-rendering: optimizeQuality; }     /*Quality*/   
#supersized li { display:block; list-style:none; z-index:-30; position:fixed; overflow:hidden; top:60px; left:0; width:100%; height:auto; background:transparent; }
